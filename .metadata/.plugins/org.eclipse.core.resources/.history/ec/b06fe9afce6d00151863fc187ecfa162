package code;

public class Edge extends TileFeature {
	
	
	// constructor
	public Edge(Feature feature1, Feature feature2, Feature feature3){
		super(feature1, feature3, feature3);
		
	}
	
	@Override 
	public boolean equals(Object obj){
		
		System.out.println("start compare");
		Edge edge = (Edge) obj;
		System.out.println(" create edge");
		System.out.println(edge.getSection1() + ",   " + getSection1() );
		if (edge.getSection1()!=getSection1()){
			System.out.println("section 1");
			return false;
		}
		System.out.println(edge.getSection2() + ",   " + getSection2() );
		if (edge.getSection2()!=getSection2()){
			System.out.println("section 2");
			return false;
		}
		System.out.println(edge.getSection3() + ",   " + getSection3() );
		if (edge.getSection3()!=getSection3()){
			System.out.println("section 3");
			return false;
		}
		return true; 
		
	}
}//END class 
